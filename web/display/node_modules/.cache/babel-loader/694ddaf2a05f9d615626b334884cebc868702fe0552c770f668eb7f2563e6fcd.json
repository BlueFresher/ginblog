{"ast":null,"code":"import bannerView from \"@/components/bannerView/index\";\nimport footerView from \"@/components/footerView/index.vue\";\n// mdÊñá‰ª∂Âú∞ÂùÄ\nimport markdown from \"../home.md\";\nimport \"./css/FirstView.scss\";\nimport \"highlight.js/styles/github.css\";\nimport \"github-markdown-css\";\nexport default {\n  name: 'FirstView',\n  components: {\n    bannerView,\n    markdown,\n    footerView\n  },\n  created() {\n    this.getProfileInfo();\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.scrollToTop);\n    this.$nextTick(() => {\n      let $ele = this.$refs.banner;\n      this.bannerH = $ele.$el.offsetHeight;\n    });\n  },\n  destroyed() {\n    window.removeEventListener(\"scroll\", this.scrollToTop);\n  },\n  data() {\n    return {\n      //‰æßËæπÊ†èÂ§¥ÂÉèÂ§ßÂ∞è\n      size: 90,\n      bannerH: 0,\n      locked: false,\n      btnFlag: false,\n      //ÂØºËà™ËÉåÊôØÂõæÁâá\n      img: \"http://chaichaiimage.oss-cn-hangzhou.aliyuncs.com/blog3.0/bg16.jpg\",\n      // ÂØºËà™ÊñáÂ≠óËØ¥Êòé\n      title: \"È¶ñÈ°µ\",\n      profileInfo: {\n        id: 1\n      }\n    };\n  },\n  methods: {\n    methods: {\n      // Ëé∑Âèñ‰∏™‰∫∫ËÆæÁΩÆ\n      async getProfileInfo() {\n        const {\n          data: res\n        } = await this.$http.get(`profile/${this.profileInfo.id}`);\n        this.profileInfo = res.data;\n        this.$root.$emit('msg', res.data.icp_record);\n      }\n    },\n    backTop() {\n      const that = this;\n      let timer = setInterval(() => {\n        let ispeed = Math.floor(-that.scrollTop / 5);\n        document.documentElement.scrollTop = document.body.scrollTop = that.scrollTop + ispeed;\n        if (that.scrollTop === 0) {\n          clearInterval(timer);\n        }\n      }, 16);\n    },\n    scrollToTop() {\n      const that = this;\n      let scrollTop = window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop;\n      that.scrollTop = scrollTop;\n      that.locked = that.btnFlag = that.scrollTop > that.bannerH;\n      if (that.scrollTop > that.bannerH) {\n        that.locked = true;\n        that.btnFlag = true;\n      } else {\n        that.locked = false;\n        that.btnFlag = false;\n      }\n    }\n  }\n};","map":{"version":3,"mappings":"AAoDA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;EACAA;EACAC;IAAAC;IAAAC;IAAAC;EAAA;EACAC;IACA;EACA;EACAC;IACAC;IACA;MACA;MACA;IACA;EACA;EACAC;IACAD;EACA;EACAE;IACA;MACA;MACAC;MACAC;MACAC;MACAC;MACA;MACAC;MACA;MACAC;MACAC;QACAC;MACA;IACA;EACA;EACAC;IACAA;MACA;MACA;QACA;UAAAT;QAAA,yBACA,iCACA;QACA;QACA;MACA;IACA;IACAU;MACA;MACA;QACA;QACAC,+DACAC;QACA;UACAC;QACA;MACA;IACA;IACAC;MACA;MACA,gBACAhB,sBACAa,sCACAA;MACAC;MACAA;MACA;QACAA;QACAA;MACA;QACAA;QACAA;MACA;IACA;EACA;AACA","names":["name","components","bannerView","markdown","footerView","created","mounted","window","destroyed","data","size","bannerH","locked","btnFlag","img","title","profileInfo","id","methods","backTop","document","that","clearInterval","scrollToTop"],"sourceRoot":"src/views/FirstView","sources":["FirstView.vue"],"sourcesContent":["<!--\n * @Author: chaichai chaichai@cute.com\n * @Date: 2022-09-26 08:29:56\n * @LastEditors: chaichai chaichai@cute.com\n * @LastEditTime: 2022-10-10 17:13:00\n * @FilePath: \\blog3.0\\src\\views\\FirstView\\FirstView.vue\n * @Description:  [CQUCC-4-433](https://github.com/4-433) Ê≠£Âú®ÊâæÂØªÂøóÂêåÈÅìÂêàÁöÑÂ∞è‰ºô‰º¥ÔºåÊ¨¢ËøéÂâçÁ´Ø„ÄÅÂêéÁ´Ø„ÄÅUIÂä†ÂÖ•Êàë‰ª¨ÔºÅ\n * \n * Copyright (c) 2022 by CQUCC-4-433, All Rights Reserved. \n-->\n\n<template>\n  <div class=\"aboutBox\">\n    <bannerView\n      :imgUrl=\"this.img\"\n      :titleName=\"this.title\"\n      ref=\"banner\"\n    ></bannerView>\n    <div class=\"mainBox\">\n      <div class=\"contentBox\">\n        <div class=\"contentTitle\">\n          <div class=\"markdown-body\">\n            <markdown />\n          </div>\n        </div>\n      </div>\n      <div :class=\"locked ? 'asideBoxFix' : 'asideBox'\">\n        <div class=\"asideImg\">\n          <!-- Â§¥ÂÉè -->\n          <v-avatar\n            :src=\"profileInfo.avatar\" alt \n            :size=\"size\"\n            class=\"asidePic\">\n            <img :src=\"profileInfo.avatar\" alt />\n          </v-avatar>\n        </div>\n        <div class=\"asideTile\">{{profileInfo.name}}</div>\n        <!-- <div class=\"asideTile1\">ËÄÅÁà∑‰øù‰ΩëÔºÅÂâçÈÄîÊó†ÈáèÔºÅ</div> -->\n        <v-divider>üå¥</v-divider>\n        <!-- ‰æßËæπÊ†èÂ∫ïÈÉ®ÂõæÁâá -->\n        <img src=\"@/assets/huli.gif\" alt=\"\" class=\"bottomImg\" />\n      </div>\n      <div v-if=\"btnFlag\" class=\"go-top\" @click=\"backTop\">\n        <!-- ËøîÂõûÈ°∂ÈÉ®ÂõæÊ†á -->\n        <img src=\"@/assets/backTop.png\" alt=\"\" class=\"backTopbtn\" /> \n      </div>\n    </div>\n    <footerView></footerView>\n  </div>\n</template>\n\n<script>\nimport bannerView from \"@/components/bannerView/index\";\nimport footerView from \"@/components/footerView/index.vue\";\n// mdÊñá‰ª∂Âú∞ÂùÄ\nimport markdown from \"../home.md\";\nimport \"./css/FirstView.scss\";\nimport \"highlight.js/styles/github.css\";\nimport \"github-markdown-css\";\nexport default {\n  name:'FirstView',\n  components: { bannerView, markdown, footerView },\n  created() {\n    this.getProfileInfo()\n  },\n  mounted() {\n    window.addEventListener(\"scroll\", this.scrollToTop);\n    this.$nextTick(() => {\n      let $ele = this.$refs.banner;\n      this.bannerH = $ele.$el.offsetHeight;\n    });\n  },\n  destroyed() {\n    window.removeEventListener(\"scroll\", this.scrollToTop);\n  },\n  data() {\n    return {\n      //‰æßËæπÊ†èÂ§¥ÂÉèÂ§ßÂ∞è\n      size: 90,\n      bannerH: 0,\n      locked: false,\n      btnFlag: false,\n      //ÂØºËà™ËÉåÊôØÂõæÁâá\n      img: \"http://chaichaiimage.oss-cn-hangzhou.aliyuncs.com/blog3.0/bg16.jpg\",\n      // ÂØºËà™ÊñáÂ≠óËØ¥Êòé\n      title: \"È¶ñÈ°µ\",\n      profileInfo: {\n        id: 1\n      }\n    };\n  },\n  methods: {\n    methods: {\n        // Ëé∑Âèñ‰∏™‰∫∫ËÆæÁΩÆ\n        async getProfileInfo() {\n            const { data: res } = await this.$http.get(\n                `profile/${this.profileInfo.id}`\n        )\n        this.profileInfo = res.data\n        this.$root.$emit('msg', res.data.icp_record)\n        }\n    },\n    backTop() {\n      const that = this;\n      let timer = setInterval(() => {\n        let ispeed = Math.floor(-that.scrollTop / 5);\n        document.documentElement.scrollTop = document.body.scrollTop =\n          that.scrollTop + ispeed;\n        if (that.scrollTop === 0) {\n          clearInterval(timer);\n        }\n      }, 16);\n    },\n    scrollToTop() {\n      const that = this;\n      let scrollTop =\n        window.pageYOffset ||\n        document.documentElement.scrollTop ||\n        document.body.scrollTop;\n      that.scrollTop = scrollTop;\n      that.locked = that.btnFlag = that.scrollTop > that.bannerH\n      if (that.scrollTop > that.bannerH) {\n        that.locked = true;\n        that.btnFlag = true;\n      } else {\n        that.locked = false;\n        that.btnFlag = false;\n      }\n    },\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}