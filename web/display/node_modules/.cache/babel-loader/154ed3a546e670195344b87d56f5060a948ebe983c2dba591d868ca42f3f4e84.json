ue"],"sourcesContent":["<template>\n  <div class=\"blogBox\">\n    <div class=\"titleBox\">\n      <div class=\"titleName\">疑难杂症专区</div>\n      <div class=\"bannerBox\">\n        <slider ref=\"slider\" :options=\"options\" @slide=\"slider\">\n          <slideritem\n            v-for=\"(item, index) in someList\"\n            :key=\"index\"\n            :style=\"item.style\"\n            >{{ item.html }}<br />\n            {{ item.athuer }}<br />\n            {{ item.creatTime }}</slideritem\n          >\n        </slider>\n      </div>\n    </div>\n    <div class=\"contentBox\">\n      <div class=\"contentTile\"></div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { slider, slideritem } from \"vue-concise-slider\";\n\nexport default {\n  components: { slider, slideritem },\n  data() {\n    return {\n      someList: [\n        {\n          html: \"动画效果抖动解决\",\n          athuer: \"chaichai\",\n          creatTime: \"2021-10-19\",\n          style: {\n            background:\n              'url(\"http://chaichaiimage.oss-cn-hangzhou.aliyuncs.com/blog3.0/bg15.jpg\")',\n            backgroundSize: \"cover\",\n          },\n        },\n        {\n          html: \"Hover盒子抖动\",\n          athuer: \"chaichai\",\n          creatTime: \"2021-10-19\",\n\n          style: {\n            background:\n              'url(\"http://chaichaiimage.oss-cn-hangzhou.aliyuncs.com/blog3.0/bg16.jpg\")',\n            backgroundSize: \"cover\",\n          },\n        },\n        {\n          html: \"自定义右键菜单无法关上\",\n          athuer: \"chaichai\",\n          creatTime: \"2021-10-19\",\n\n          style: {\n            background:\n              'url(\"http://chaichaiimage.oss-cn-hangzhou.aliyuncs.com/blog3.0/bg17.jpg\")',\n            backgroundSize: \"cover\",\n          },\n        },\n      ],\n      options: {\n        currentPage: 0,\n        direction: \"vertical\",\n        slidesToScroll: 1, //每次滑动项数\n        effect: \"fade\",\n        thresholdDistance: 10,\n        loop: true,\n      },\n    };\n  },\n  methods: {\n      slider(data) {\n        console.log(data,'silder3');\n      },\n  },\n};\n</script>\n\n<style lang='scss'>\n.blogBox {\n  display: flex;\n  flex-wrap: nowrap;\n  width: 100%;\n  height: 800px;\n  box-shadow: 0 1px 10px rgb(0 0 0 / 10%);\n  margin-bottom: 20px;\n  .titleBox {\n    width: 35%;\n    height: 100%;\n    text-align: center;\n    .titleName {\n      margin: 0 auto;\n      width: 80%;\n      font-size: 24px;\n      height: 50px;\n      line-height: 50px;\n      font-weight: 550;\n    }\n    .bannerBox {\n      width: 100%;\n      height: 750px;\n      .slider-item {\n        color: rgb(255, 255, 255);\n        font-weight: 550;\n        line-height: 100px;\n        text-shadow: 2px 2px 2px #000;\n        font-size: 30px;\n      }\n    }\n  }\n  .contentBox {\n    width: 65%;\n    height: 100%;\n    background: skyblue;\n  }\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}