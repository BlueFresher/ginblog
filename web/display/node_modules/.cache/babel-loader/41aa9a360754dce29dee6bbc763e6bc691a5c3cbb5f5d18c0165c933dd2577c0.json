  <div class=\"topTitle\">BlueFresher.top</div>\n          <!-- <el-menu\n          class=\"el-menu-demo\"\n          mode=\"horizontal\"\n          @select=\"handleSelect\"\n          collapse-transition\n          background-color=\"#0000001D\"\n          router\n          text-color=\"#fff\"\n          menu-trigger=\"click\"\n        > -->\n        \n        <div class=\"el-menu-demo\" background-color=\"#0000001D\" text-color=\"#fff\">\n          <v-btn text index=\"about\" @click=\"$router.push('/about')\"><h1>首页</h1></v-btn>\n          <v-btn text index=\"blog\" @click=\"$router.push('/blog')\"><h1>博客</h1></v-btn>\n          <v-btn text index=\"back\" @click=\"$router.push('/back')\"><h1>后台</h1></v-btn>\n          <v-btn text index=\"back\" @click=\"$router.push('/back')\"><h1>注册</h1></v-btn>\n          <v-dialog max-width=\"800\">\n            <template v-slot:activator=\"{on, attrs}\" text-color=\"#fff\" background-color=\"#0000001D\">\n              <v-btn class=\"hidden-md-and-down\" text @click=\"loginout\"><h1>退出</h1></v-btn>\n              <v-btn text v-bind=\"attrs\" v-on=\"on\"><h1>登录</h1></v-btn>\n            </template>\n\n            <template v-slot:default=\"dialog\" class=\"ground\">\n              <div class=\"main-box\">\n                <div :class=\"['container', 'container-register', { 'is-txl': isLogin }]\">\n                  <form>\n                    <h2 class=\"title\">Create Account</h2>\n                    <span class=\"text\">or use email for registration</span>\n                    <input class=\"form__input\" type=\"text\" placeholder=\"Name\" />\n                    <input class=\"form__input\" type=\"text\" placeholder=\"Email\" />\n                    <input class=\"form__input\" type=\"password\" placeholder=\"Password\" />\n                    <div class=\"primary-btn\">立即注册</div>\n                  </form>\n                </div>\n                <div\n                  :class=\"['container', 'container-login', { 'is-txl is-z200': isLogin }]\"\n                >\n                  <form>\n                    <h2 class=\"title\">Sign in to Website</h2>\n                    <span class=\"text\">or use email for registration</span>\n                    <input class=\"form__input\" type=\"text\" placeholder=\"Email\" />\n                    <input class=\"form__input\" type=\"password\" placeholder=\"Password\" />\n                    <div class=\"primary-btn\">立即登录</div>\n                  </form>\n                </div>\n                <div :class=\"['switch', { login: isLogin }]\">\n                  <div class=\"switch__circle\"></div>\n                  <div class=\"switch__circle switch__circle_top\"></div>\n                  <div class=\"switch__container\">\n                    <h2>{{ isLogin ? 'Hello Friend !' : 'Welcome Back !' }}</h2>\n                    <p>\n                      {{\n                        isLogin\n                          ? 'Enter your personal details and start journey with us'\n                          : 'To keep connected with us please login with your personal info'\n                      }}\n                    </p>\n                    <div class=\"primary-btn\" @click=\"isLogin = !isLogin\">\n                      {{ isLogin ? '立即注册' : '立即登录' }}\n                    </div>\n                  </div>\n                </div>\n              </div>\n            </template>\n            \n          </v-dialog>\n        </div>\n          <!-- <template v-slot:activator=\"{ on, attrs }\"> -->\n\n          <!-- <v-menu offset-y> \n          <template v-slot:activator=\"{ on, attrs }\">\n            <v-btn text color=\"white\" index=\"about\" v-bind=\"attrs\" v-on=\"on\"><h1>查看友链</h1></v-btn>\n          </template>\n            <v-list>\n              <v-list-item v-for=\"(item, index) in items\" :key=\"index\">\n                <v-list-item-title >\n                  <img :src=\"item.img\" alt=\"\" class=\"friendIco\" /><a\n                  :href=\"item.href\"\n                  style=\"color: #fff\"\n                  >百梦</a>\n                </v-list-item-title>\n              </v-list-item>\n            </v-list> -->\n            <!-- <el-submenu index=\"2\">\n              <template slot=\"title\">友链</template>\n              <el-menu-item index=\"2-1\" class=\"friendList\"\n                ><img src=\"@/assets/baimeng.png\" alt=\"\" class=\"friendIco\" /><a\n                  href=\"https://marrydream.top/\"\n                  style=\"color: #fff\"\n                  >百梦</a\n                ></el-menu-item\n              >\n              <el-menu-item index=\"2-2\" class=\"friendList\">\n                <a href=\"#\" style=\"color: #fff\">虚位以待~</a>\n              </el-menu-item>\n            </el-submenu> -->\n          <!-- </v-menu> -->\n        <!-- </el-menu> -->\n        </div>\n        <div class=\"centerTile\">{{ titleName }}</div>\n        <div class=\"icon\">﹀</div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// import Vue from 'vue'\n// import ElementUI from 'element-ui';\n\n// Vue.use(ElementUI);\nexport default {\n  name:'bannerView',\n  // components :{ ElementUI } ,\n  ccc:'',\n  props: {\n    imgUrl: {\n      required: true,\n    },\n    titleName: {\n      required: true,\n    },\n  },\n  data() {\n    return {\n      activeIndex: \"1\",\n      items:[\n        {\n          img: \"@/assets/baimeng.png\",\n          href: \"https://marrydream.top/\"\n        }\n      ],\n      formdata: {\n            username:'',\n            password:''\n      },\n      checkPassword: '',\n      dialog: false,\n    };\n  },\n  mounted() {\n    ccc = this.cc()\n  },\n  methods: {\n    handleSelect(key, keyPath) {\n      console.log(key, keyPath);\n    },\n    cc() {\n      // return rgba(255,255,255,0);\n    }\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.ground{\n  display: none;\n  \n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\n  width: 400px;\n  height: 250px;\n  border-radius: 20px;\n  border: 2px solid #fff;\n  background: #fff;\n  margin: auto;\n  position: absolute;\n  top: -200px;\n  left: 0;\n  right: 0;\n  bottom: 0;\n}\n\n.form_input {\n  width: 350px;\n  height: 40px;\n  margin: 4px 0;\n  padding-left: 25px;\n  font-size: 13px;\n  letter-spacing: 0.15px;\n  border: none;\n  outline: none;\n  // font-family: 'Montserrat', sans-serif;\n  background-color: #ecf0f3;\n  transition: 0.25s ease;\n  border-radius: 8px;\n  box-shadow: inset 2px 2px 4px #d1d9e6, inset -2px -2px 4px #f9f9f9;\n  &::placeholder {\n    color: #a0a5a8;\n  }\n}\n\n.bannerBox {\n  width: 100vw;\n  height: 100vh;\n  background-size: cover;\n  overflow: hidden;\n}\n.coverBox {\n  width: 100%;\n  height: 100%;\n  background: rgba(0, 0, 0, 0.4);\n\n  .navBox {\n    height: 60px;\n    display: flex;\n    flex-wrap: nowrap;\n    line-height: 60px;\n    justify-content: space-between;\n    padding: 20px 35px 0 35px;\n\n    .topTitle {\n      width: 300px;\n      text-align: left;\n      color: #fff;\n      font-size: 38px;\n      font-weight: 900;\n      text-transform: uppercase;\n    }\n    .el-menu {\n      background-color: rgb(0, 0, 0, 0) !important;\n      border: 0px;\n    }\n\n    .el-menu-item {\n      font-size: 18px;\n      font-weight: 600;\n      background-color: rgb(0, 0, 0, 0) !important;\n    }\n    .el-submenu__title {\n      font-size: 18px !important;\n      font-weight: 600;\n      background-color: rgb(0, 0, 0, 0) !important;\n    }\n  }\n  .centerTile {\n    width: 100%;\n    line-height: 70vh;\n    color: #fff;\n    font-size: 38px;\n    font-weight: 900;\n    letter-spacing: 8px;\n  }\n  .icon {\n    margin-top: 30px;\n    z-index: 99999;\n    font-weight: 900;\n    font-size: 35px;\n    color: #fff;\n  }\n}\n</style>\n<style>\n.friendIco {\n  display: inline-block;\n  width: 25px;\n  margin: 0 3px;\n  vertical-align: middle;\n}\n.friendList {\n  height: 80px !important;\n  line-height: 80px !important;\n  margin: 5px auto;\n  text-align: center;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}